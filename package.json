{
  "name": "securian.insurance.web18",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --browsers ChromeHeadless",
    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\"  --fix",
    "format": "npx prettier \"src/**/*.{js,jsx,ts,tsx,html,css,scss}\" --write",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/demoapp/stats-es2015.json"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{js,ts,scss,md,html,json}": [
      "prettier --write",
      "npx eslint --fix --write",
      "lint",
      "git add"
    ]
  },

